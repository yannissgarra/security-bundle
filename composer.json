{
    "name": "webmunkeez/security-bundle",
    "type": "symfony-bundle",
    "description": "JWT based security for Symfony.",
    "keywords": [
        "symfony",
        "autowiring",
        "security",
        "provider",
        "token",
        "jwt"
    ],
    "homepage": "https://github.com/yannissgarra/security-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannis Sgarra",
            "email": "hello@yannissgarra.com",
            "homepage": "https://yannissgarra.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/security-bundle": "^6.0",
        "firebase/php-jwt": "^6.1",
        "symfony/validator": "^6.0",
        "egulias/email-validator": "^3.1",
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.0",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/framework-bundle": "^6.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Webmunkeez\\SecurityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmunkeez\\SecurityBundle\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
